<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="unifyum">

	<resultMap id="RM.arrangepermis"
		class="com.bupt.qrj.unifyum.dal.dataobject.arrangepermisDO">

        <result property="set_start_time" column="mission_set_start_time" javaType="java.lang.String"
                jdbcType="varchar" nullValue="0"/>
		<result property="set_start_time_code" column="set_start_time_code" javaType="java.lang.String"
			    jdbcType="varchar" nullValue="0"/>
        <result property="mission_condition" column="mission_condition" javaType="java.lang.String"
                jdbcType="varchar" nullValue="0"/>
        <result property="mission" column="mission" javaType="java.lang.String"
                jdbcType="varchar" nullValue="0"/>
		<result property="worker_name" column="worker_name" javaType="java.lang.String"
				jdbcType="varchar" nullValue="0"/>


	</resultMap>

	<select id="arrange-per-mis" resultMap="RM.arrangepermis">
		select mission_set_start_time,mission_condition,mission,worker_name from mission where set_start_time &lt;= #timemax# and set_start_time &gt;= #timemin# and worker_name = #worker_name# and set_start_time_code = #set_start_time_code#;
	</select>

